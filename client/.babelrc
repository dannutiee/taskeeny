{
    "presets": ["@babel/env", "@babel/react", "@babel/typescript"],
    "plugins": [
      
        ["styled-components",
          {
            "displayName": true,
            "ssr": false
          }],
        "transform-class-properties",
        "transform-object-rest-spread",
        [
          "@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ]
        
      ]
}



